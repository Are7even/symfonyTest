                                        <li><a href="{{ path('app_category_index') }}">Categories</a></li>
                                    </ul>
                                </li>
                            </ul>
                        </nav>
                    </div> <!-- Header Main Menu Start -->
                </div>
            </div>
        </div>
    </div> <!-- End Bottom Area -->
</header> <!-- ...:::: End Header Section:::... -->
