    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
        {{ encore_entry_link_tags('font-awesome') }}
        {{ encore_entry_link_tags('plaza-icon') }}
        {{ encore_entry_link_tags('jquery-ui') }}
        {{ encore_entry_link_tags('slick') }}
        {{ encore_entry_link_tags('nice-select') }}
        {{ encore_entry_link_tags('venobox') }}
    {% endblock %}

    {% block javascripts %}
